start: s
accept: ac
reject: rj
blank: e

s 0 -> add_ 0 <
s 1 -> add_ 1 <
s 2 -> add_ 2 <
add_ e -> begin _ >

begin 0 -> even 0 > 
begin 1 -> odd 1 >
begin 2 -> even 2 >

even 0 -> even 0 >
even 1 -> odd 1 >
even 2 -> even 2 >
even e -> push0 e <

odd 0 -> odd 0 >
odd 1 -> even 1 >
odd 2 -> odd 2 >
odd e -> push1 e <

push0 0 -> push0 0 <
push0 1 -> push0 1 <
push0 2 -> push0 2 <
push0 _ -> push0 _ <
push0 e -> to_begin 0 >

push1 0 -> push1 0 <
push1 1 -> push1 1 <
push1 2 -> push1 2 <
push1 _ -> push1 _ <
push1 e -> to_begin 1 >

to_begin 0 -> to_begin 0 >
to_begin 1 -> to_begin 1 >
to_begin 2 -> to_begin 2 >
to_begin _ -> q _ >

q 0 -> q 0 >
q 1 -> qc 0 >
q 2 -> q 1 >
q e -> check e <

qc 0 -> qc 1 >
qc 1 -> q 2 >
qc 2 -> qc 2 >
qc e -> check e <

check 0 -> check 0 <
check 1 -> cont 1 <
check 2 -> cont 2 <
check _ -> output _ ^

cont 0 -> cont 0 <
cont 1 -> cont 1 <
cont 2 -> cont 2 <
cont _ -> begin _ >

output _ -> output e >
output 0 -> output e >
output e -> jmp_num e ^

jmp_num 0 -> jmp_begin 0 <
jmp_num 1 -> jmp_begin 1 <
jmp_num 2 -> jmp_begin 2 <
jmp_num e -> jmp_num e <

jmp_begin 0 -> jmp_begin 0 <
jmp_begin 1 -> jmp_begin 1 <
jmp_begin 2 -> jmp_begin 2 <
jmp_begin e -> ac e >

